    "homepage": "https://modrinth.com/mod/trial-spawner-timer",
    "issues": "https://github.com/Diamondgoobird/TrialSpawnerTimer/issues"
  },
  "license": "LGPL-3.0-only",
  "icon": "assets/trialchambertimer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.diamondgoobird.trialchambertimer.TrialSpawnerTimer"
    ]
  },
  "mixins": [
    "trialspawnertimer.mixins.json"
  ],
  "depends": {
